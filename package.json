{
  "name": "teslavault",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "db:generate": "pnpm --filter @teslavault/api db:generate",
    "db:push": "pnpm --filter @teslavault/api db:push",
    "db:migrate": "pnpm --filter @teslavault/api db:migrate",
    "db:studio": "pnpm --filter @teslavault/api db:studio"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.4.0"
  }
}
